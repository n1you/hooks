{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "typeRoots": [
      "src/types/*.d.ts"
    ],
    "paths": {
      "@@/*": [
        ".dumi/tmp/*"
      ],
      "Al-hooks": [
        "src/"
      ],
      "Al-hooks/*": [
        "src/*",
        "*"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "src/useAsyncMemo",
    "src/useCheckOnly",
    "src/useExecuteOnce",
    "src/useFuzzySearchSort",
    "src/useState",
    "src/useWebWorker",
    "src/utils",
    "src/index.ts",
  ]
}